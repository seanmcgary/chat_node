:@template"ı/* Welcome to Compass.
 * In this file you should write your main styles. (or centralize your imports)
 * Import this file using the following HTML or equivalent:
 * <link href="/stylesheets/screen.css" media="screen, projection" rel="stylesheet" type="text/css" /> */

@import "compass/reset";

body {
    margin: 0px 0px 0px 0px;
    padding: 0px 0px 0px 0px;
    font-family: Helvetica, Arial, sans-serif;
    background: #eeeeee;
    
}

input[type="text"], input[type="password"] {
    margin:  0px 0px 0px 0px;
    padding:  0px 0px 0px 0px;
    height: 25px;
    border:  1px solid #cccccc;
    background:  #fff;
    min-width: 250px;
}

input[type="submit"] {
    margin:  0px 0px 0px 0px;
    padding:  5px 5px 5px 5px;
    border-radius: 5px;
    border:  none;
}

input[type="submit"].green {
    background: #7fd67f;
    color: #fff;
}

input[type="submit"].green:hover {
    background: #509450;
    color: #fff;
    cursor: pointer;
}

.div-row {
    margin:  0px 0px 10px 0px;
    padding:  0px 0px 0px 0px;
    overflow: hidden;


    .field-label {
        margin:  0px 0px 0px 0px;
        padding:  0px 0px 0px 0px;
        width: 120px;
        position: relative;
        float: left;
    }

    .field-value {
        margin:  0px 0px 0px 0px;
        padding:  0px 0px 0px 0px;
        position: relative;
        float: left;
    }
}

.login {
    margin:  30px auto 0px auto;
    padding:  10px 10px 10px 10px;
    background:  #fff;
    width: 380px;
    border-radius: 10px;
    overflow: hidden;
    /*display: none;*/
}

.status-bubble {
    margin: 2px 5px 0px 0px;
    height: 10px;
    border-radius: 5px;
    width: 10px;
    position: relative;
    float: left;
}

.green-bubble {
    background: #008000;
}

.red-bubble {
    background: #ff0000;
}

.yellow-bubble {
    background: #ffff00;
}

.gray-bubble {
    background: #808080;
}

.toolbar {
    margin: 0px 0px 0px 0px;
    padding: 0px 0px 0px 0px;
    height: 30px;
    background: #003366;
    position: relative;
    clear: bottom;
    color: #eeeeee;
    font-size: .8em;

    .header-text {
        margin: 0px 0px 0px 0px;
        padding: 10px 0px 0px 0px;
    }

    ul.tabs-list {
        margin:  0px 0px 0px 0px;
        padding:  0px 0px 0px 0px;
        list-style: none;

        li {
            margin:  0px 0px 0px 0px;
            padding:  0px 5px 2px 5px;
            position: relative;
            float: left;
            background:  #cccccc;
            color: #434343;
            border-top-left-radius: 5px;
            border-top-right-radius: 5px;
            font-size: .8em;
            border-top:  1px solid #fff;
            border-left:  1px solid #fff;
            border-right:  1px solid #fff;

            .text {
                margin: 0px 0px 0px 0px;
                padding:  5px 0px 0px 0px;
                position: relative;
                float: left;
            }

            .close {
                margin:  3px 0px 0px 5px;
                padding:  0px 0px 0px 0px;
                background: #6d8eaf;
                border: 1px solid #fff;
                position: relative;
                float: left;
                text-align: center;
                width: 15px;
                height: 15px;
                line-height: 1em;
                color: #fff;
            }
        }
    }
}

.chat {
    margin:  0px 0px 0px 0px;
    padding:  0px 0px 0px 0px;
    min-width: 960px;
    display: none;
    height: 100%;

    .chat-container {
        margin:  0px 0px 0px 0px;
        padding:  0px 0px 0px 0px;
        overflow: hidden;

        .contacts {
            margin:  0px 0px 0px 0px;
            padding:  0px 0px 0px 0px;
            width: 229px;
            float: right;
            background: #fff;
            border-left: 1px solid #003366;
            border-bottom: 1px solid #003366;

            .list-container {

                
                .list-header {
                    margin: 0px 0px 0px 0px;
                    padding: 5px 5px 5px 5px;
                    background: #cccccc;
                    font-size: .9em;
                    color: #434343;

                }
                ul.contacts-list {
                    margin:  0px 0px 0px 0px;
                    padding:  0px 0px 0px 0px;
                    list-style: none;
                    overflow-y: scroll;


                    li {
                        margin:  0px 0px 5px 0px;
                        padding:  5px 5px 5px 5px;
                        list-style: none;
                        font-size: .7em;

                    }
                }

            }

            .list-container:not(:last-of-type) ul.contacts-list{
                border-bottom: 1px solid #cccccc;
            }
        }

        .current-content {
            margin:  0px 0px 0px 0px;
            padding:  0px 0px 0px 0px;
            overflow: hidden;
            background:  #eeeeee;
            border-bottom: 1px solid #003366;
            border-right: 1px solid #003366;

            .chat-box {
                margin:  0px 0px 0px 0px;
                padding:  0px 0px 0px 0px;
                background: #fff;
                overflow: hidden;
                
                .chat-text {
                    margin:  0px 0px 0px 0px;
                    padding:  5px 5px 5px 5px;
                    //border:  1px solid #cccccc;
                    overflow-y: scroll;
                    
                    .chat-line {
                        margin:  0px 0px 0px 0px;
                        padding:  2px 2px 2px 2px;
                        position: relative;
                        font-size: .8em;
                        
                        .chat-meta {
                            margin:  2px 0px 2px 0px;
                            padding:  0px 0px 0px 0px;
                            color: #cccccc;
                            font-style: italic;
                            font-size: .7em;
                        }
                    }
                }
            }

            .chat-input {
                margin:  0px 0px 0px 0px;
                padding:  10px 10px 10px 10px;
                background: #eeeeee;
                overflow: hidden;
                
                .chat-submit {
                    margin:  0px 0px 0px 0px;
                    padding:  0px 0px 0px 0px;
                    width: 65px;
                    float: right;

                    input[type="submit"] {
                        margin: 0px 0px 0px 0px;
                        padding: 5px 14px 6px;
                        background-color: #0064CD;
                        background-repeat: repeat-x;
                        background-image: -moz-linear-gradient(top, #049cdb, #0064cd);
                        background-image: -webkit-linear-gradient(top, #049cdb, #0064cd);
                        background-image: linear-gradient(top, #049cdb, #0064cd);
                        text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
                        border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
                        border: 1px solid #CCC;
                        border-radius: 4px;
                        box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2), 0 1px 2px rgba(0, 0, 0, 0.05);
                        color: #fff;
                    }
                }

                .text-box {
                    margin:  0px 75px 0px 0px;
                    padding:  0px 0px 0px 0px;
                    background: red;

                    input.chat-message {
                        margin:  0px 0px 0px 0px;
                        padding:  5px 0px 5px 0px;
                        width: 100%;
                        border:  1px solid #b3b2b2;
                        background: #fff;

                    }
                }
            }
        }
    }

